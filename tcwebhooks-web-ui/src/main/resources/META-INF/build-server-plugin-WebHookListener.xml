		<property name="rank">
			<value>100</value>
		</property>
	</bean>
	
	<bean id="slackComCompactXmlWebHookTemplate"
		class="webhook.teamcity.payload.template.SlackComCompactXmlWebHookTemplate"
		init-method="register">
